{
  "collectCoverage": true,
  "collectCoverageFrom" : [
    "src/**/*.{js,jsx}",
    "!**/node_modules/**",
    "!src/**/*.test.{js,jsx}"
  ],
  "moduleNameMapper": {
    ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/src/__mocks__/cssModule.js",
    ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/image.js"
  }
}
